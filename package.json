{
  "name": "alloycrop-vue",
  "version": "1.1.0",
  "description": "AlloyCrop vue wrap",
  "main": "dist/alloy-crop-vue.umd.js",
  "module": "dist/alloy-crop-vue.esm.js",
  "unpkg": "dist/alloy-crop-vue.min.js",
  "browser": {
    "./sfc": "src/alloy-crop-vue.vue"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "alloyfinger": "^0.1.16",
    "css3transform": "^1.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "exif-js": "^2.3.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l-ll/alloy-crop-vue.git"
  },
  "author": "l-ll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/l-ll/alloy-crop-vue/issues"
  },
  "homepage": "https://github.com/l-ll/alloy-crop-vue#readme"
}
